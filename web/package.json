{
  "name": "specmate",
  "version": "0.0.1",
  "description": "Specmate for co-engineering requirements and tests",
  "scripts": {
    "extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/de.json ./src/assets/i18n/gb.json -m ngx_trans_extr --sort --format namespaced-json",
    "lint": "tslint ./src/**/*.ts -t verbose --exclude './src/app/model/**/*.ts'",
    "clean": "rimraf ../bundles/specmate-ui-core/webcontent/*",
    "pack-dev": "webpack --config webpack/webpack.dev.js",
    "pack-prod": "webpack --config webpack/webpack.prod.js",
    "build-dev": "npm run extract && npm run lint && npm run clean && npm run pack-dev",
    "build-prod": "npm run clean && npm run pack-prod",
    "dep-graph": "ngd -p src/tsconfig.json",
    "init": "npm prune && npm install",
    "license-check": "license-checker --summary --production",
    "update-check": "ncu",
    "check": "npm run lint && npm run license-check && npm run update-check",
    "update-dependencies": "ncu -u && npm run init",
    "test": "mocha -r ts-node/register -r ignore-styles src/**/*.spec.ts"
  },
  "keywords": [],
  "author": "Technical University of Munich - Maximilian Junker and Sebastian Eder",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@ng-bootstrap/ng-bootstrap": "3.2.2",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/jquery": "^3.3.4",
    "angular-split": "1.0.0-rc.3",
    "angular2-uuid": "1.1.1",
    "bootstrap": "4.1.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "flag-icon-css": "3.2.0",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "ng2-dragula": "2.1.0",
    "ngx-cookie": "^4.0.2",
    "popper.js": "1.14.4",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "typeface-open-sans": "0.0.54",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/yargs": "^12.0.1",
    "angular2-dependencies-graph": "1.0.0-alpha.12",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "9.1.5",
    "awesome-typescript-loader": "5.2.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "git-revision-webpack-plugin": "3.0.3",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "^5.0.1",
    "license-checker": "20.2.0",
    "loader-utils": "1.1.0",
    "mocha": "^5.2.0",
    "node-sass": "4.9.3",
    "npm-check-updates": "2.14.2",
    "postcss-loader": "3.0.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "string-replace-loader": "2.1.1",
    "style-loader": "0.23.0",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.1.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-merge": "4.1.4"
  },
  "repository": {}
}
